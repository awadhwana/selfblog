<h1 align="center">Listing all articles</h1>
<%= link_to "Create Article", new_article_url, style: 'margin: 50px'%>
<div align="center">
<%= will_paginate %>
</div>
<h5 align="center">Categories</h5>
<div align="center"; id="category">
    <ul class='list-inline'>
        <% Category.all.each do |c| %>
            <li class='list-inline-item'>
            <%= link_to c.name, category_path(c), :remote => true %>    
            </li>
            <li class='list-inline-item'>
              |
            </li>
        <% end %>
    </ul>
    </div>
    <div id="filter"></div>
<%= render 'article', obj: @articles %>
<div align="center">
<%= will_paginate %>
</div>